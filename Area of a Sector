#include <stdio.h>
#define PI 3.14159

int main() {
    float radius, angle;
    printf("Enter the radius of the sector: ");
    scanf("%f", &radius);
    printf("Enter the angle in degrees of the sector: ");
    scanf("%f", &angle);
    float area = (angle / 360.0) * PI * radius * radius;
    printf("Area of the sector: %f\n", area);
    return 0;
}
