#include <stdio.h>

int main() {
    float base, height;
    printf("Enter the base length of the parallelogram: ");
    scanf("%f", &base);
    printf("Enter the height of the parallelogram: ");
    scanf("%f", &height);
    float area = base * height;
    printf("Area of the parallelogram: %f\n", area);
    return 0;
}
